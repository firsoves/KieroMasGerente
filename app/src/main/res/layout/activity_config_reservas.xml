<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".config_reservas">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/kiero_mas13"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="6dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/svg_top7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kiero_mas" />

    <androidx.cardview.widget.CardView
        android:id="@+id/view"
        class="androidx.appcompat.widget.Toolbar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/Blanco"
        android:elevation="10dp"
        app:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.07"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView102"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/circulo"
        android:backgroundTint="@color/principal_app"
        android:elevation="10dp"
        android:onClick="barra_button_menu"
        android:outlineSpotShadowColor="@color/principal_app"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.985" />

    <ImageView
        android:id="@+id/imageView103"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:elevation="10dp"
        android:tint="@color/secundario_app"
        app:layout_constraintBottom_toBottomOf="@+id/imageView102"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView102"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/icon_pedido" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView104"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="10dp"
        android:onClick="barra_button_tickets"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView102"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintWidth_percent="0.08"
        app:srcCompat="@drawable/ic_baseline_list_alt_24" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView105"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="10dp"
        android:onClick="barra_button_tickets"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView102"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintWidth_percent="0.08"
        app:srcCompat="@drawable/camion_de_reparto" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/svg_top7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/barra_de_arribarecurso"
        android:backgroundTint="@color/secundario_app"
        android:elevation="7dp"
        android:tint="@color/secundario_app"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.8" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.04" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.96" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="32dp"
        android:layout_height="0dp"
        android:elevation="11dp"
        android:tint="@color/secundario_app"
        app:layout_constraintBottom_toBottomOf="@+id/svg_top7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_notifications_black_24dp" />

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="30dp"
        android:layout_height="0dp"
        android:elevation="11dp"
        android:tint="@color/secundario_app"
        app:layout_constraintBottom_toBottomOf="@+id/svg_top7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_pages_24" />

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/svg_top7">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ToggleButton
                android:id="@+id/toggleButton2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:checked="true"
                android:text="ToggleButton"
                android:textColorHint="@color/secundario_app"
                android:textColorLink="@color/principal_app"
                android:textOff="Reservas DESACTIVADAS"
                android:textOn="Reservas Activadas"
                android:textSize="30sp" />

            <TextView
                android:id="@+id/textView96"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:fontFamily="@font/nunito_bold"
                android:gravity="center"
                android:text="Selecciona las Horas Reservables"
                android:textSize="20dp"
                app:autoSizeTextType="uniform" />

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginEnd="32dp"
                android:orientation="vertical">

                <Button
                    android:id="@+id/button24"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#FFC4C4"
                    android:fontFamily="@font/nunito_light"
                    android:text="00:00-01:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button25"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#FFC4C4"
                    android:fontFamily="@font/nunito_light"
                    android:text="01:00-02:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button26"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#FFC4C4"
                    android:fontFamily="@font/nunito_light"
                    android:text="02:00-03:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button33"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#FFC4C4"
                    android:fontFamily="@font/nunito_light"
                    android:text="03:00-04:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button27"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#FFC4C4"
                    android:fontFamily="@font/nunito_light"
                    android:text="04:00-05:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button28"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#FFC4C4"
                    android:fontFamily="@font/nunito_light"
                    android:text="05:00-06:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button32"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#FFC4C4"
                    android:fontFamily="@font/nunito_light"
                    android:text="06:00-07:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button34"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#FFC4C4"
                    android:fontFamily="@font/nunito_light"
                    android:text="07:00:08:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button35"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="08:00-09:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button43"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="09:00-10:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button42"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="10:00-11:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button36"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="11:00-12:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button37"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="12:00-13:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button38"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="13:00-14:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button39"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="14:00-15:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button40"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="15:00-16:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button41"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="16:00-17:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button44"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:backgroundTint="#CEFFBB"
                    android:fontFamily="@font/nunito_light"
                    android:text="17:00-18:00"
                    android:textColor="@color/common_google_signin_btn_text_light_pressed"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/button45"
